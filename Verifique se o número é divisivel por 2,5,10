//Entrar com um número e informar se ele é ou não divisível por 2,5,10.

import java.util.*;
class Main {
  public static void main(String[] args) {
    double n1;

    Scanner scan = new Scanner(System.in);
    
    System.out.println("Entre com um número");
    n1 = scan.nextDouble();

    if (n1 % 5 == 0){
      System.out.println("Este número é divisivel por 5");
    } else if (n1 % 10 == 0) {
      System.out.println("Este número é divisivel por 10");
    } else if (n1 % 2 == 0) {
      System.out.println("Este número é divisivel por 2");
    } else {
        System.out.println("Este número não é divisivel por nenhum deles.");
    }
   
    

    
  }
}
